        <fullName>Account_Merged</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
Account.MergeStatus__c = &apos;Merged&apos;, 
$Profile.Name &lt;&gt; &apos;System Administrator&apos;, 
$User.CommunityNickname &lt;&gt; &apos;DealerSiteGuestUser&apos;, 
$Profile.Name &lt;&gt; $Label.Vgtna_System_Integrator_Profile_Name 
)</errorConditionFormula>
        <errorMessage>Merged Account - Unable to Save (Cancel).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lost_Reason_Detail_Mandatory</fullName>
        <active>true</active>
        <description>Lost Reason Detail is mandatory when Lost Reason is Price or Other.</description>
        <errorConditionFormula>AND( OR( ISPICKVAL( Lost_Reason__c , &quot;Other&quot;), ISPICKVAL( Lost_Reason__c , &quot;Price&quot;)), ISBLANK( Lost_Reason_Detail__c ))</errorConditionFormula>
        <errorDisplayField>Lost_Reason_Detail__c</errorDisplayField>
        <errorMessage>Lost Reason Detail is mandatory when Lost Reason is Price or Other.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lost_Reason_mandatory_for_Lost_Oppty</fullName>
        <active>true</active>
        <description>When Stagename= &quot;Closed/Lost&quot; then Lost Reason is mandatory</description>
        <errorConditionFormula>AND( ISPICKVAL( StageName , &quot;Closed Lost&quot;),ISPICKVAL( Lost_Reason__c , &quot;&quot;)  )</errorConditionFormula>
        <errorDisplayField>Lost_Reason__c</errorDisplayField>
        <errorMessage>If opportunity is &quot;Closed Lost&quot;, &quot;Lost Reason&quot; is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lost_Reason_on_Closed_Lost_Only</fullName>
        <active>true</active>
        <description>Lost Reason and Lost Reason Details should not be populated on any opportunity that is not Closed Lost.</description>
        <errorConditionFormula>AND( NOT( ISPICKVAL( StageName , &quot;Closed Lost&quot;) ) ,
OR( NOT( ISBLANK( Lost_Reason_Detail__c ) ) ,
NOT(ISBLANK(TEXT( Lost_Reason__c )))
)
)</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>Lost Reason and Lost Reason Detail should not be populated when Opportunity Stage is not &quot;Closed Lost&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Mandatory_Fields_for_Quoted_Stage</fullName>
        <active>true</active>
        <description>Mack Mandatory Fields for Quoted Stage</description>
        <errorConditionFormula>AND(
ISPICKVAL (StageName, &quot;Quoted&quot; ),
OR(
RecordType.DeveloperName = &quot;Other&quot;
),
OR (
ISPICKVAL(Brand__c, &quot;&quot;) , ISBLANK( Make__c ),
ISPICKVAL(Segment__c, &quot;&quot;)  )
)</errorConditionFormula>
        <errorMessage>Brand, Make and Segment are required for Quoted stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Volvo_Mandatory_Fields_for_Quoted_Stage</fullName>
        <active>true</active>
        <description>Fields needed for quoted stage</description>
        <errorConditionFormula>AND(
 ISPICKVAL (StageName, &quot;Quoted&quot; ),
OR( 
RecordType.DeveloperName =  &quot;Volvo&quot;,
RecordType.DeveloperName = &quot;Mack&quot; 
),
OR (
ISPICKVAL(Brand__c, &quot;&quot;) , 
ISPICKVAL(Segment__c, &quot;&quot;) ,  ISPICKVAL (Primary_Model__c,&quot;&quot;) )
)</errorConditionFormula>
        <errorMessage>Brand, Segment and Primary Model are required for Quoted Stage.</errorMessage>
    </validationRules>
</CustomObject>
