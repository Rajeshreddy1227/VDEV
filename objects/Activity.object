    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Account_Type__c</fullName>
        <externalId>false</externalId>
        <label>Account Type</label>
        <length>18</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Account__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Account</label>
        <referenceTo>Account</referenceTo>
        <relationshipLabel>Activities</relationshipLabel>
        <relationshipName>Activities</relationshipName>
        <required>false</required>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>ActivityStatus__c</fullName>
        <externalId>false</externalId>
        <formula>IF(IsTask,TEXT(Status),IF(NOW() &gt; End_Date__c, &apos;Completed&apos;, &apos;Open&apos;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Activity Status</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Activity_Type__c</fullName>
        <externalId>false</externalId>
        <label>Activity Type</label>
        <required>false</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>true</sorted>
                <value>
                    <fullName>Call</fullName>
                    <default>false</default>
                    <label>Call</label>
                </value>
                <value>
                    <fullName>Email</fullName>
                    <default>false</default>
                    <label>Email</label>
                </value>
                <value>
                    <fullName>Event</fullName>
                    <default>false</default>
                    <label>Event</label>
                </value>
                <value>
                    <fullName>Task</fullName>
                    <default>false</default>
                    <label>Task</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Brand__c</fullName>
        <externalId>false</externalId>
        <label>Brand</label>
        <required>false</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Mack</fullName>
                    <default>false</default>
                    <label>Mack</label>
                </value>
                <value>
                    <fullName>Volvo</fullName>
                    <default>false</default>
                    <label>Volvo</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Completed_Date__c</fullName>
        <externalId>false</externalId>
        <formula>IF(IsTask,CompletedDateTime,IF(AND(NOT(IsTask),NOW() &gt; End_Date__c),End_Date__c,null))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Completed Date</label>
        <required>false</required>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>CreatedBy_Profile__c</fullName>
        <externalId>false</externalId>
        <label>CreatedBy Profile</label>
        <length>30</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Dealer_Code__c</fullName>
        <externalId>false</externalId>
        <label>Dealer Code</label>
        <length>20</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>EmailNotification__c</fullName>
        <defaultValue>false</defaultValue>
        <description>user has to set this flag to see email notofications. This should be available in tasks.</description>
        <externalId>false</externalId>
        <label>Email notification upon task completion</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>End_Date__c</fullName>
        <externalId>false</externalId>
        <label>End Date</label>
        <required>false</required>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Event_Type__c</fullName>
        <externalId>false</externalId>
        <label>Event Type</label>
        <length>100</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>External_ID__c</fullName>
        <caseSensitive>false</caseSensitive>
        <externalId>true</externalId>
        <label>External ID</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>IsCompleted__c</fullName>
        <externalId>false</externalId>
        <formula>IF(IsTask,TEXT(Status)= &apos;Completed&apos;,IF(AND(NOT(IsTask),NOW() &gt; End_Date__c),True,False))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>IsCompleted</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>IsOutlookEvent__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <label>IsOutlookEvent</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Outlook_Orgin__c</fullName>
        <externalId>false</externalId>
        <label>Outlook Orgin</label>
        <length>15</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Task_Owner_Name__c</fullName>
        <description>Owner Name to use in Email Templates</description>
        <externalId>false</externalId>
        <formula>Owner:User.FirstName + &quot; &quot; + Owner:User.LastName</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Task Owner Name</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>isBrand__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT($User.Dealer_Brand__c) = TEXT(Brand__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>isBrand</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>isDealerCode__c</fullName>
        <externalId>false</externalId>
        <formula>$User.Dealer_Code__c = Dealer_Code__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>isDealerCode</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>isDealerManager__c</fullName>
        <externalId>false</externalId>
        <formula>Owner:User.Dealer_Manager__r.User_Id__c = $User.User_Id__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>isDealerManager</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>isExecutiveGroup__c</fullName>
        <externalId>false</externalId>
        <formula>CONTAINS($User.Executive_group__c, Dealer_Code__c)</formula>
        <label>isExecutiveGroup</label>
        <type>Checkbox</type>
    </fields>
    <listViews>
        <fullName>MyActivities</fullName>
        <columns>ACTIVITY_SUBTYPE</columns>
        <columns>TASK.STATUS</columns>
